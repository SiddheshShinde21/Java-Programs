// Check the number is prime or not 
import java.util.*;

public class Prime{
public static void main(String args[]){
  Scanner sc = new Scanner(System.in);
  boolean isPrime = true;
  System.out.println("Enter any number:-");
  int n = sc.nextInt();
  for (int i = 2; i*i<=n;i++){
    if (n % i == 0){
      isPrime = false;
      break;
    }
  }
System.out.println(isPrime);
}
}
