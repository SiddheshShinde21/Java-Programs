// Calculate the sum of odd numbers from 1 to N
import java.util.*;
public class SumOfOdd {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter any number- ");
        int n = sc.nextInt();
        int oddSum = 0;
        for (int i = 1; i<=n;i++){
            if(i % 2 != 0){     
                oddSum +=i;
            }
        }
        System.out.println("Sum = "+oddSum);  
    }
}
